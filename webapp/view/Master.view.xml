                                <Column minScreenWidth="Tablet" demandPopin="true">
                                    <Text text="{i18n>idocNum}" />
                                </Column>
                                <Column minScreenWidth="Tablet" demandPopin="true">
                                    <Text text="{i18n>docVent}" />
                                </Column>
                                <Column minScreenWidth="Tablet" demandPopin="true">
