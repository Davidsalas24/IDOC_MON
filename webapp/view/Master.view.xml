                                    <core:ListItem key="{servicesModel>Tipex}" text="{servicesModel>Ttipex}" additionalText="{servicesModel>Tipex}"/>
                                </Select> -->
                                <DateRangeSelection placeholder=" "></DateRangeSelection>
                            </fb:control>
                        </fb:FilterGroupItem>
                        <fb:FilterGroupItem groupName="Cli" visibleInFilterBar="true" name="NunInter" label="{i18n>numInterCli}">
                            <fb:control>
                                <Input value="" />
                            </fb:control>
                        </fb:FilterGroupItem>

                        <fb:FilterGroupItem groupName="Planta" visibleInFilterBar="true" name="Plant" label="{i18n>plant}">
                            <fb:control>
                                <Input value="" />
                            </fb:control>
                        </fb:FilterGroupItem>
                        
                    </fb:filterGroupItems>
                </fb:FilterBar>
            </f:DynamicPageHeader>
			</f:header>
